{
  "name": "apollo-ng",
  "version": "17.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development",
    "watch": "ng build --watch --configuration development",
    "start:preprod": "ng serve --configuration preprod",
    "build:preprod": "ng build --configuration preprod && npm run postbuild:preprod",
    "build": "ng build --configuration production && npm run postbuild",
    "test": "ng test",
    "postbuild": "node scripts/generate-sitemap.mjs https://dspay.fr && node scripts/generate-sitemap-index.mjs https://dspay.fr",
    "generate:sitemap": "node scripts/generate-sitemap.mjs https://dspay.fr",
    "generate:sitemap:index": "node scripts/generate-sitemap-index.mjs https://dspay.fr",
     "e2e": "cypress run",
     "e2e:open": "cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.5",
    "@angular/cdk": "^17.0.2",
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/forms": "^17.0.5",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "@angular/router": "^17.0.5",
    "@fullcalendar/angular": "^6.0.3",
    "@fullcalendar/core": "^6.0.3",
    "@fullcalendar/daygrid": "^6.0.3",
    "@fullcalendar/interaction": "^6.0.3",
    "@fullcalendar/timegrid": "^6.0.3",
    "@stripe/stripe-js": "^7.9.0",
    "chart.js": "^3.3.2",
    "libphonenumber-js": "^1.12.22",
    "primeflex": "^3.3.1",
    "primeicons": "6.0.1",
    "primeng": "17.2.0",
    "quill": "^2.0.3",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.12",
    "@angular/cli": "^19.2.17",
    "@angular/compiler-cli": "^17.0.5",
    "@ngtools/webpack": "^17.3.12",
    "@types/jasmine": "~4.3.1",
    "@types/node": "^24.6.2",
    "css-loader": "^6.11.0",
    "cypress": "^15.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "mini-css-extract-plugin": "^2.9.4",
    "sitemap": "^8.0.0",
    "typescript": "~5.2.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4"
  }
}
